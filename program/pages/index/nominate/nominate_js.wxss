@import "../../../common/iconFont.wxss";
.nominate{
  width: 100%;
  height: auto;
}
.nom_title{
  width: 100%;
  height: 30px;
  margin-bottom: 16px;
  border-bottom: 1px solid #D5D4D4;
}

.nom_text2{
  margin-left: 15px;
  width: 60px;
  height: 30px;
  line-height: 30px;
  font-size: 14px;
  font-family:PingFang-SC-Regular;
  font-weight:bold;
  color:rgba(40,40,40,1);
}


.nom_main{
  width: 92%;
  height: auto;
  margin:  0 auto;
  display: flex;
  display: -webkit-flex;
  flex-direction: row;
  flex-wrap: wrap;
  justify-content: space-between;
}
.main{
  width: 160px;
  height: auto;
}
.nom_img{
  width: 160px;
  height: 160px;
  /* margin-left: 15px; */
  border-radius:3px;
}
.nom_view{
  width: 160px;
  height: auto;
  font-size: 0;
  display: flex;
  box-sizing: border-box;
  justify-content: space-between;
}

.nom_left{
  margin-left: 10px;
  margin-top: 10px;
  height: auto;
  display: flex;
  display: -webkit-flex;
  flex-direction: column;
  flex-wrap: wrap;
  margin-bottom: 10px;
}
.adress{
  margin-top: 10px;
  display: flex;
  
}
.adress text{
  font-size: 10px;
}
.main_text1{
  
  height: 14px;
  line-height: 14px;
  font-size: 14px;
  font-family:PingFang-SC-Regular;
  font-weight:bold;
  color:rgba(40,40,40,1);
}
.main_text2{
 
  height: 10px;
  line-height: 10px;
  font-size: 10px;
  font-family:PingFang-SC-Regular;
  font-weight:bold;
  color:rgba(40,40,40,1);
  margin-top: 10px;
}
