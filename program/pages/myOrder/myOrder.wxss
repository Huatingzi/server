.jiantou{
  width: 43px;
  height: 27px;
  position: fixed;
  right: 0;
  top: 130px;
  z-index: 9999;
}
.jiantou image{
  width: 100%;
  height: 100%;
}
.myOrder{
  background: #FAF9F9;
  width: 100%;
  height: 100%;
   
}
.myOrder text{
  color: #282828;
}
.tab{
  display: flex;
}
.tab text{
 display: block;
 text-align: center;
 width: 25%;
 padding: 17px 0;
 color: #9D9C9C;
 font-size:12px;
 border-bottom:1px solid #D5D4D4;
}
.tab .active{
  border-bottom: 1px solid #61B7F9;
  color: #61B7F9;
}
.tabItem text{
word-break:keep-all;           /* 不换行 */
white-space:nowrap;     
}

.tabItem{
  background: #fff;
  display: flex;
  align-items: center;
  margin: 15px 15px 30px 15px;
  border-radius: 5px;
  box-shadow:0 0 12px rgba(0, 0, 0, 0.13);
  max-height: 207px;
  position: relative;
  transform: translateX(0px);
  transition: all .4s;
}
.tabItem image{
  width:160px;
  height: 103px;
  margin: 15px;
  background: #ccc;
  border-radius: 2px;
  
}
.tabItem .orderDetail{
  display: flex;
  flex-direction: column;
  width: 147px;
  
  padding-right:16px;
  margin-top: 15px;
  
}
.tabItem .orderDetail .top{
  display: flex;
  justify-content: space-between;
  font-weight: bold;
  border-bottom: 1px solid #D5D4D4;
}
.tabItem .orderDetail .top text:nth-child(1){
  color: #60B7F9;
  font-size: 15px;
  
}
.tabItem .orderDetail .top text:nth-child(2){
  font-size: 13px;
}
.tabItem .orderDetail .content{
  display: flex;
  flex-direction: column;
}
.tabItem .orderDetail .content .Servicetime{
  display: flex;
  margin-top: 8px 
}
.tabItem .orderDetail .content .Servicetime text{
  font-size: 10px;
}
.content .Servicetime text:nth-child(1){
  margin-right: 20px;
}
.content .time{
  display: flex;
  align-items: center;
}
.content .time text{
 font-size: 12px;
}
.content .bottom{
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.content .price{
  margin-top: 10px;
  margin-bottom: 15px;
}
.content .price .money {
  font-weight: bold;
  font-size: 13px;
  color: #FE0202;
  display: block;
  
}

.content   navigator{
  width: 60px;
  height: 21px;
  line-height: 21px;
  border-radius: 2px;
  font-size: 10px;
  padding:0;
  margin-top:10px;
  margin-bottom: 15px;
  
  text-align: center;
  background: #60B7F9;
  color: #fff;

}


