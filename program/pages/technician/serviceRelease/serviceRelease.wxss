.ser_service2{
  width: auto;
  height: auto;
  margin-left: 15px;
  display: flex;
  display: -webkit-flex;
  flex-direction: row;
  flex-wrap: wrap;
  justify-content: space-between;
}
.ser_service2 image{
  position: relative;
  width: 120px;
  height: 120px;
  margin: 5px;
  border: 1px solid #000;
}
.ser_service2 image:before,.ser_service2 image:after{
  content: '';
  position: absolute;
  left: 50%;
  top: 50%;
  background: red;
  
  /* margin-top: -8px; */
}
.ser_service2 image:before{
  width: 50px;
  height: 2px;
  margin-left: -25px;
}
.ser_service2 image:after{
  width: 2px;
  height: 50px;
  /* margin-left: -8px; */
  margin-top: -25px;
}


