<view>
  <block wx:for="{{list}}" wx:key="unique">
  <view class='list'>
    <view class="put">
    <text>{{item.type_name}} {{item.status == 1 ? '已到账' : '未到账'}}</text>
    </view>
    <view class="money">
      <text >{{item.money}}</text>
    </view>
    <view class="time">
      <text >{{item.add_time}}</text>
    </view>
  </view>
  </block>
</view>
